{
  "name": "kirby-starthilfe",
  "version": "0.1.0",
  "description": "An opinionated Kirby 4 website boilerplate",
  "type": "module",
  "scripts": {
    "cache:clear": "rm -rf storage/cache",
    "twsort": "rustywind --write site",
    "dev": "npm run cache:clear && rm -rf public/dist && vite",
    "build": "npm run cache:clear && rm -rf public/dist && vite build"
  },
  "keywords": [
    "Kirby",
    "Kirby 4",
    "Vite",
    "Typescript"
  ],
  "author": "Lasse Weinbrandt",
  "private": true,
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "laravel-vite-plugin": "^1.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-blade": "^2.1.21",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2",
    "vite": "^5.4.19"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild"
    ]
  },
  "packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971"
