{
  "name": "kirby-starthilfe",
  "version": "0.1.0",
  "description": "An opinionated Kirby 4 website boilerplate",
  "type": "module",
  "scripts": {
    "cache:clear": "rm -rf storage/cache",
    "twsort": "rustywind --write site",
    "dev": "npm run cache:clear && rm -rf public/dist && vite",
    "build": "npm run cache:clear && rm -rf public/dist && vite build"
  },
  "keywords": [
    "Kirby",
    "Kirby",
    "4",
    "Vite",
    "Typescript"
  ],
  "author": "Lasse Weinbrandt",
  "private": true,
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^20.16.5",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.6.2",
    "vite": "^5.4.6",
    "vite-plugin-kirby": "^5.3.1"
  }
}
